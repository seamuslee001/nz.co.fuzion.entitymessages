<?xml version="1.0"?>
<extension key="nz.co.fuzion.entitymessages" type="module">
  <file>entitymessages</file>
  <name>Entity Messages</name>
  <description>Extension allows saving messages against an entity. These can be used in tokens.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>eileen</author>
    <email>eileen@fuzion.co.nz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/eileenmcnaughton/nz.co.fuzion.entitymessages</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">https://github.com/eileenmcnaughton/nz.co.fuzion.entitymessages/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2016-01-07</releaseDate>
  <version>1.0</version>
  <compatibility>
    <ver>4.6</ver>
    <ver>4.7</ver>
  </compatibility>
  <comments>The initial focus is on domain messages - allowing text blocks to be set up for use in tokens</comments>
  <civix>
    <namespace>CRM/Entitymessages</namespace>
  </civix>
</extension>
